{
  "name": "@immoassist/profil",
  "version": "1.0.0",
  "description": "Profil module - User profile and settings",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "lint": "echo 'No linting for profil package'",
    "test": "echo 'No tests for profil package'",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@immoassist/ui": "workspace:*",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "restricted"
  }
}
